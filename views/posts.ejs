<h1>게시글 목록</h1>

<ul>
    <% posts.forEach(post=> { %>
        <li>
            <h3>
                <%=post.title %>
            </h3>
            <p>
                <%=post.desc %>
            </p>
            <p>
                <%=post.author.username %>
            </p>

        </li>
        <% }) %>
</ul>

<a href="/">홈으로</a>